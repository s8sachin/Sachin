<h1>Listing  movie</h1>
<table  border="solid ">
	<tr>
		<th>movie title</th>
		<th>movie year</th>
		<th>movie rating</th>
		<th>movie language</th>
	</tr>
	
	<tr>
		<td><%= @movie.title %></td>
		<td><%= @movie.year %></td>
		<td><%= @movie.rating %></td>
		<td><%= @movie.language %></td>
		<td><%= link_to "list users" %> </td>
		<td><%= link_to "Create users" %> </td>
		<td><%= link_to "Edit", edit_movie_path(@movie)%> </td>
		<td><%= link_to "Destroy", movie_path(@movie), method: :delete, data: {
		confirm: "Are you sure?"
		}%> </td>
	</tr>
	
</table>